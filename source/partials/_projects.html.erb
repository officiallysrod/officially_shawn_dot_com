<main class="projects-container" id="projects">
  <section>
    <article class="type-system-sans">
      <p class="type">Projects</p>
      <h1>Some Things I've Built</h1>
    </article>

    <div class="projects xflex-boxes">
      <% data.projects.each_with_index do |project, index| %>
        <%= partial "partials/project", locals: {project: project, index: index} %>
      <% end %>
    </div>
  </section>
</main>
